{
  "elements": [
    {
      "draw": [
        { "type": "drawCircle", "x": 200, "y": 300, "radius": 50, "color": "#FF0000" },
        { "type": "drawText", "x": 200, "y": 360, "text": "Hello", "color": "#FFFFFF", "fontSize": 40 }
      ],
      "hit": [
        { "type": "circleHitTest", "x": 200, "y": 300, "radius": 50 }
      ],
      "onTap": [
        { "type": "toast", "message": "Circle tapped!" },
        { "type": "script", "code": "toast('JS Tap!')" }
      ]
    },
    {
      "draw": [
        { "type": "drawRect", "x": 100, "y": 500, "width": 150, "height": 100, "color": "#00FF00" }
      ],
      "hit": [
        { "type": "rectHitTest", "x": 100, "y": 500, "width": 150, "height": 100 }
      ],
      "onTap": [
        { "type": "log", "message": "Rect tapped!" }
      ]
    }
  ]
}
