{
  "id": "home_screen",
  "title": "Welcome to Sillia",
  "components": [
    {
      "type": "box",
      "x": 40,
      "y": 100,
      "width": 300,
      "height": 80,
      "color": "#6200EE",
      "text": "Go to Profile",
      "actions": [
        {
          "type": "navigate_to",
          "payload": {
            "url": "https://sillia.pages.dev/profile.json"
          }
        }
      ]
    },
    {
      "type": "box",
      "x": 40,
      "y": 220,
      "width": 300,
      "height": 80,
      "color": "#03DAC5",
      "text": "Open Website",
      "actions": [
        {
          "type": "open_url",
          "payload": {
            "url": "https://sillia.pages.dev/"
          }
        }
      ]
    },
    {
      "type": "box",
      "x": 40,
      "y": 340,
      "width": 300,
      "height": 80,
      "color": "#FF5722",
      "text": "Show Toast",
      "actions": [
        {
          "type": "toast",
          "payload": {
            "message": "Welcome to Sillia!"
          }
        }
      ]
    }
  ]
}
