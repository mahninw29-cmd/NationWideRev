{
  "elements": [
    {
      "draw": "circle",
      "props": {
        "x": 200,
        "y": 200,
        "radius": 100,
        "color": "#FF0000",
        "hitScript": "console.log('Circle tapped!')"
      },
      "onTap": [
        {
          "type": "script",
          "code": "console.log('Circle onTap executed')"
        }
      ]
    },
    {
      "draw": "rect",
      "props": {
        "x": 50,
        "y": 400,
        "width": 300,
        "height": 150,
        "color": "#00FF00",
        "hitScript": "console.log('Rectangle tapped!')"
      },
      "onTap": [
        {
          "type": "script",
          "code": "console.log('Rectangle onTap executed')"
        }
      ]
    },
    {
      "draw": "line",
      "props": {
        "x1": 100,
        "y1": 100,
        "x2": 400,
        "y2": 400,
        "strokeWidth": 8,
        "color": "#0000FF"
      }
    },
    {
      "draw": "text",
      "props": {
        "x": 100,
        "y": 600,
        "value": "Hello Compose!",
        "color": "#FFFFFF",
        "fontSize": 50
      }
    },
    {
      "draw": "custom",
      "props": {
        "script": "console.log('Custom element executed')"
      }
    }
  ]
}
