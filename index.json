{
  "commands": [
    {
      "type": "draw",
      "action": "drawRect",
      "x": 50,
      "y": 300,
      "width": 300,
      "height": 60,
      "color": "#4285F4",
      "radius": 12,
      "onClick": {
        "type": "google_signin",
        "successNavigate": "profile_screen"
      }
    },
    {
      "type": "draw",
      "action": "drawText",
      "text": "Continue with Google",
      "x": 150,
      "y": 335,
      "color": "#FFFFFF",
      "fontSize": 20,
      "bold": true,
      "align": "center"
    },
    {
      "type": "draw",
      "action": "drawImage",
      "src": "https://upload.wikimedia.org/wikipedia/commons/5/53/Google_%22G%22_Logo.svg",
      "x": 70,
      "y": 310,
      "width": 40,
      "height": 40
    },

    {
      "type": "layout",
      "id": "profile_screen",
      "commands": [
        {
          "type": "draw",
          "action": "drawCircle",
          "x": 200,
          "y": 200,
          "radius": 60,
          "color": "#CCCCCC",
          "imageUrl": "{profileUrl}" 
        },
        {
          "type": "draw",
          "action": "drawText",
          "text": "{name}",
          "x": 200,
          "y": 300,
          "color": "#000000",
          "fontSize": 24,
          "bold": true,
          "align": "center"
        },
        {
          "type": "draw",
          "action": "drawText",
          "text": "{email}",
          "x": 200,
          "y": 340,
          "color": "#666666",
          "fontSize": 18,
          "align": "center"
        }
      ]
    }
  ]
}
